{
  "kind": "collectionType",
  "collectionName": "resource_templates",
  "info": {
    "singularName": "resource-template",
    "pluralName": "resource-templates",
    "displayName": "Resource templates"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "key": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "role": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "PM",
        "Dev",
        "Design",
        "QA"
      ]
    },
    "daily_cost": {
      "type": "decimal"
    },
    "daily_rate": {
      "type": "integer"
    },
    "is_active": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "meta_data": {
      "type": "json"
    }
  }
}
